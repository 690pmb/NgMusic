{
  "name": "ng-music",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 7070",
    "build": "ng build --configuration production --progress false",
    "lint": "run-s lint:**",
    "lint:format": "prettier --check .",
    "lint:stylelint": "stylelint \"**/*.scss\" verbose",
    "lint:ts": "eslint --max-warnings 0 .",
    "fix": "run-s fix:**",
    "fix:format": "prettier --write .",
    "fix:stylelint": "stylelint --fix \"**/*.scss\" verbose",
    "fix:fix": "eslint --max-warnings 0 --fix .",
    "compile": "tsc --noEmit",
    "postinstall": "run-s postinstall:*",
    "postinstall:husky": "husky install",
    "postinstall:compile": "pnpm compile",
    "pretest": "pnpm compile",
    "posttest": "pnpm lint"
  },
  "engines": {
    "pnpm": ">=8",
    "node": ">=16.20.0 <20.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~16.2.12",
    "@angular/animations": "~16.2.12",
    "@angular/cdk": "16.2.14",
    "@angular/common": "~16.2.12",
    "@angular/compiler": "~16.2.12",
    "@angular/core": "~16.2.12",
    "@angular/forms": "~16.2.12",
    "@angular/material": "~16.2.14",
    "@angular/platform-browser": "~16.2.12",
    "@angular/platform-browser-dynamic": "~16.2.12",
    "@angular/service-worker": "~16.2.12",
    "@fortawesome/angular-fontawesome": "~0.13",
    "@fortawesome/fontawesome-common-types": "~6.5",
    "@fortawesome/fontawesome-svg-core": "~6.5",
    "@fortawesome/free-brands-svg-icons": "~6.5",
    "@fortawesome/free-regular-svg-icons": "~6.5",
    "@fortawesome/free-solid-svg-icons": "~6.5",
    "core-js": "3.36.0",
    "crypto-browserify": "^3.12.0",
    "dexie": "2.0.4",
    "dropbox": "10.34.0",
    "jszip": "3.10.1",
    "luxon": "~3.4.0",
    "rxjs": "7.8.1",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "tslib": "2.6.2",
    "util": "^0.12.5",
    "xml2js": "0.6.2",
    "xmlbuilder": "15.1.1",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "~16.3",
    "@angular-eslint/eslint-plugin-template": "~16.3",
    "@angular-eslint/template-parser": "~16.3",
    "@angular/cli": "~16.2.12",
    "@angular/compiler-cli": "~16.2.12",
    "@angular/language-service": "~16.2.12",
    "@types/luxon": "~3.4",
    "@types/node": "~16.18",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "~5.62.0",
    "@typescript-eslint/parser": "~5.62.0",
    "eslint": "~8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "gts": "~5.2.0",
    "husky": "~8.0.3",
    "lint-staged": "~13.3.0",
    "npm-run-all2": "5.0.2",
    "postcss": "~8.4.33",
    "postcss-sass": "0.5.0",
    "stylelint": "~15.11.0",
    "stylelint-config-sass-guidelines": "~10.0.0",
    "stylelint-config-standard": "~33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "~5.1"
  },
  "lint-staged": {
    "*.{ts, html}": "eslint --max-warnings 0 --fix",
    "*": "prettier --ignore-unknown --write",
    "*.scss": "stylelint --fix"
  }
}
