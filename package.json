{
  "name": "ng-music",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 7070",
    "build": "ng build --configuration production --progress false",
    "cordova": "npm run build --base-href file:///android_asset/www/",
    "lint": "run-p lint:**",
    "lint:ts": "gts lint",
    "lint:format": "prettier --check .",
    "lint:stylelint": "stylelint \"**/*.scss\" verbose",
    "fix": "run-s fix:*",
    "fix:format": "prettier --write .",
    "fix:stylelint": "stylelint --fix \"**/*.scss\" verbose",
    "fix:fix": "gts fix",
    "clean": "gts clean",
    "compile": "tsc --noEmit",
    "prepare": "run-s prepare:*",
    "prepare:husky": "husky install",
    "prepare:compile": "pnpm compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "12.2.18",
    "@angular/animations": "12.2.17",
    "@angular/cdk": "12.2.13",
    "@angular/common": "12.2.17",
    "@angular/compiler": "12.2.17",
    "@angular/core": "12.2.17",
    "@angular/forms": "12.2.17",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "12.2.17",
    "@angular/platform-browser-dynamic": "12.2.17",
    "@fortawesome/angular-fontawesome": "0.9.0",
    "@fortawesome/fontawesome-common-types": "0.2.36",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "core-js": "3.27.1",
    "dexie": "2.0.4",
    "dropbox": "4.0.30",
    "jszip": "3.10.1",
    "moment-mini-ts": "2.20.1",
    "rxjs": "6.6.7",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "tslib": "2.4.1",
    "xml2js": "0.4.23",
    "xmlbuilder": "15.1.1",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "~14.4",
    "@angular/cli": "12.2.18",
    "@angular/compiler-cli": "12.2.17",
    "@angular/language-service": "12.2.17",
    "@types/node": "16.18.11",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "^5",
    "codelyzer": "6.0.2",
    "eslint": "^8.41.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "gts": "~4.0",
    "husky": "~8.0",
    "lint-staged": "~13.2",
    "npm-run-all": "4.1.5",
    "postcss": "^8",
    "postcss-sass": "0.5.0",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "26.0.0",
    "typescript": "~4.3"
  },
  "lint-staged": {
    "*.ts": "eslint --max-warnings 0 --fix --no-ignore",
    "*": "prettier --ignore-unknown --write",
    "*.scss": "stylelint --fix"
  }
}
