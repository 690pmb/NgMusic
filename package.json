{
  "name": "ng-music",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 7070",
    "build": "ng build --configuration production --progress false",
    "lint": "run-s lint:**",
    "lint:format": "prettier --check .",
    "lint:stylelint": "stylelint \"**/*.scss\" verbose",
    "lint:ts": "eslint --max-warnings 0 .",
    "fix": "run-s fix:**",
    "fix:format": "prettier --write .",
    "fix:stylelint": "stylelint --fix \"**/*.scss\" verbose",
    "fix:fix": "eslint --max-warnings 0 --fix .",
    "compile": "tsc --noEmit",
    "postinstall": "run-s postinstall:*",
    "postinstall:husky": "husky",
    "postinstall:compile": "pnpm compile",
    "pretest": "pnpm compile",
    "posttest": "pnpm lint"
  },
  "engines": {
    "pnpm": ">=9",
    "node": ">=18.18 <20.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~17.3.11",
    "@angular/animations": "~17.3.12",
    "@angular/cdk": "17.3.10",
    "@angular/common": "~17.3.12",
    "@angular/compiler": "~17.3.12",
    "@angular/core": "~17.3.12",
    "@angular/forms": "~17.3.12",
    "@angular/material": "~17.3.10",
    "@angular/platform-browser": "~17.3.12",
    "@angular/platform-browser-dynamic": "~17.3.12",
    "@angular/service-worker": "~17.3.12",
    "@fortawesome/angular-fontawesome": "~0.14.1",
    "@fortawesome/fontawesome-common-types": "~6.7.2",
    "@fortawesome/fontawesome-svg-core": "~6.7.2",
    "@fortawesome/free-brands-svg-icons": "~6.7.2",
    "@fortawesome/free-regular-svg-icons": "~6.7.2",
    "@fortawesome/free-solid-svg-icons": "~6.7.2",
    "core-js": "3.39.0",
    "crypto-browserify": "^3.12.1",
    "dexie": "~3.2.7",
    "dropbox": "10.34.0",
    "jszip": "3.10.1",
    "luxon": "~3.5.0",
    "rxjs": "7.8.1",
    "stream": "0.0.3",
    "timers": "0.1.1",
    "tslib": "2.8.1",
    "util": "^0.12.5",
    "xml2js": "0.6.2",
    "xmlbuilder": "15.1.1",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "~17.5.3",
    "@angular-eslint/eslint-plugin-template": "~17.5.3",
    "@angular-eslint/template-parser": "~17.5.3",
    "@angular/cli": "~17.3.11",
    "@angular/compiler-cli": "~17.3.12",
    "@angular/language-service": "~17.3.12",
    "@stylistic/stylelint-config": "^2.0.0",
    "@types/luxon": "~3.4.2",
    "@types/node": "~18.19.69",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "~8.19.0",
    "@typescript-eslint/parser": "~8.19.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "gts": "~6.0.2",
    "husky": "^9.1.7",
    "lint-staged": "~15.3.0",
    "npm-run-all2": "~7.0.2",
    "postcss": "~8.4.49",
    "postcss-sass": "0.5.0",
    "stylelint": "~16.12.0",
    "stylelint-config-sass-guidelines": "~12.1.0",
    "stylelint-config-standard": "~36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "~5.4.5"
  },
  "lint-staged": {
    "*.{ts, html}": "eslint --max-warnings 0 --fix",
    "*": "prettier --ignore-unknown --write",
    "*.scss": "stylelint --fix"
  },
  "packageManager": "pnpm@9.15.2"
}
