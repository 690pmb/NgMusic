{
  "name": "ng-music",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 7070",
    "build": "ng build --configuration production --progress false",
    "cordova": "pnpm build --base-href file:///android_asset/www/",
    "lint": "run-s lint:**",
    "lint:format": "prettier --check .",
    "lint:stylelint": "stylelint \"**/*.scss\" verbose",
    "lint:ts": "gts lint",
    "fix": "run-s fix:**",
    "fix:format": "prettier --write .",
    "fix:stylelint": "stylelint --fix \"**/*.scss\" verbose",
    "fix:fix": "gts fix",
    "clean": "gts clean",
    "compile": "tsc --noEmit",
    "postinstall": "run-s postinstall:*",
    "postinstall:husky": "husky install",
    "postinstall:compile": "pnpm compile",
    "pretest": "pnpm compile",
    "posttest": "pnpm lint"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "15.2.8",
    "@angular/animations": "15.2.9",
    "@angular/cdk": "15.2.9",
    "@angular/common": "15.2.9",
    "@angular/compiler": "15.2.9",
    "@angular/core": "15.2.9",
    "@angular/forms": "15.2.9",
    "@angular/material": "15.2.9",
    "@angular/platform-browser": "15.2.9",
    "@angular/platform-browser-dynamic": "15.2.9",
    "@fortawesome/angular-fontawesome": "0.12.1",
    "@fortawesome/fontawesome-common-types": "6.1.0",
    "@fortawesome/fontawesome-svg-core": "6.1.0",
    "@fortawesome/free-regular-svg-icons": "6.1.0",
    "@fortawesome/free-solid-svg-icons": "6.1.0",
    "core-js": "3.31.0",
    "dexie": "2.0.4",
    "dropbox": "4.0.30",
    "jszip": "3.10.1",
    "moment-mini-ts": "2.20.1",
    "rxjs": "6.6.7",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "tslib": "2.5.3",
    "xml2js": "0.5.0",
    "xmlbuilder": "15.1.1",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "~14.4.0",
    "@angular/cli": "15.2.8",
    "@angular/compiler-cli": "15.2.9",
    "@angular/language-service": "15.2.9",
    "@types/node": "16.18.34",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "~5.59.11",
    "@typescript-eslint/parser": "~5.59.11",
    "eslint": "~8.42.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "gts": "~4.0.1",
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2",
    "npm-run-all": "4.1.5",
    "postcss": "~8.4.24",
    "postcss-sass": "0.5.0",
    "stylelint": "~15.7.0",
    "stylelint-config-sass-guidelines": "~10.0.0",
    "stylelint-config-standard": "~33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "~4.9.5"
  },
  "lint-staged": {
    "*.ts": "eslint --max-warnings 0 --fix --no-ignore",
    "*": "prettier --ignore-unknown --write",
    "*.scss": "stylelint --fix"
  }
}
